<login_screen>:
	BoxLayout:
		Image:
			source: "./assets/backgrounds/1background.png"
	BoxLayout:
		Image:
			source: "./assets/backgrounds/1cat.png"
	BoxLayout:
		Image:
			source: "./assets/backgrounds/1grumpytitle.png"
	GridLayout:
		cols: 1
		row_force_default: True
		row_default_height: 30
		spacing: 10
		BoxLayout:
		BoxLayout:
		BoxLayout:
		BoxLayout:
		BoxLayout:
		BoxLayout:
			Label:
				text: "User Name"
				size_hint: (0.4, 1)
				color: (0,0,0,1)
			TextInput:
				id:username
				multiline:False
				size_hint: (0.5, 1)
			Label:
				size_hint: (0.1, 1)
		BoxLayout:
			Label:
				text:"Password"
				size_hint: (0.4, 1)
				color: (0,0,0,1)
			TextInput:
				id:password
				multiline: False
				password: True
				size_hint: (0.5, 1)
			Label:
				size_hint: (0.1, 1)
		BoxLayout:
			Label:
				id:status
				color: (0,0,0,1)
		BoxLayout:
			Label:
				size_hint: (0.05, 1)
			Button:
				text: "Log In"
				size_hint: (0.425, 1)
				color: (0,0,0,1)
				background_normal: "./assets/buttons/long_button.png"
				background_down: "./assets/buttons/long_button_down.png"
				on_press:
					root.manager.transition.direction = 'left'
					root.log_in()
			Label:
				size_hint: (0.05, 1)
			Button:
				text: "Sign Up"
				color: (0,0,0,1)
				size_hint: (0.425, 1)
				background_normal: "./assets/buttons/long_button.png"
				background_down: "./assets/buttons/long_button_down.png"
				on_press:
					root.manager.transition.direction = 'left'
					root.manager.current = 'signup'
			Label:
				size_hint: (0.05, 1)


<signup_screen>:
	BoxLayout:
		Image:
			source: "./assets/backgrounds/1background.png"
	BoxLayout:
		Image:
			source: "./assets/backgrounds/1cat.png"
	BoxLayout:
		Image:
			source: "./assets/backgrounds/1grumpytitle.png"
	GridLayout:
		id: sign_up
		cols: 1
		row_force_default: True
		row_default_height: 30
		spacing: 10
		BoxLayout:
		BoxLayout:
		BoxLayout:
		BoxLayout:
		BoxLayout:
		BoxLayout:
			Label:
				text: "User Name"
				size_hint: (0.4, 1)
				color: (0,0,0,1)
			TextInput:
				id: username
				multiline: False
				size_hint: (0.55, 1)
			Label:
				size_hint: (0.05, 1)
		BoxLayout:
			Label:
				text: "Email"
				size_hint: (0.4, 1)
				color: (0,0,0,1)
			TextInput:
				id: email
				multiline: False
				size_hint: (0.55, 1)
			Label:
				size_hint: (0.05, 1)
		BoxLayout:
			Label:
				text: "Password"
				size_hint: (0.4, 1)
				color: (0,0,0,1)
			TextInput:
				id: password
				multiline: False
				password: True
				size_hint: (0.55, 1)
			Label:
				size_hint: (0.05, 1)
		BoxLayout:
			Label:
				text: "Confirm Password"
				size_hint: (0.4, 1)
				color: (0,0,0,1)
			TextInput:
				id: password_confirm
				multiline: False
				password: True
				size_hint: (0.55, 1)
			Label:
				size_hint: (0.05, 1)
		BoxLayout:
			Label:
				id:status
				color: (0,0,0,1)
		BoxLayout:
			Button:
				text: "Sign Me Up!"
				color: (0,0,0,1)
				background_normal: "./assets/buttons/long_button.png"
				background_down: "./assets/buttons/long_button_down.png"
				on_press:
					root.manager.transition.direction = 'left'
					root.sign_me_up()
		BoxLayout:
			Button:
				text: "<< Back to Login"
				color: (0,0,0,1)
				background_normal: "./assets/buttons/long_button.png"
				background_down: "./assets/buttons/long_button_down.png"
				on_press:
					root.manager.transition.direction = 'right'
					root.manager.current = 'login'

